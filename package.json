{
  "name": "stylelint-config-yoctol",
  "version": "0.7.0",
  "description": "Yoctol's stylelint config",
  "author": "Yoctol Info.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoctol/stylelint-config-yoctol.git"
  },
  "main": "index.js",
  "files": ["index.js"],
  "scripts": {
    "testonly": "jest",
    "lint": "eslint __tests__ index.js",
    "test": "npm run lint && npm run testonly"
  },
  "dependencies": {
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-yoctol-base": "^0.15.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "prettier-package-json": "^1.6.0",
    "stylelint": "^9.2.0"
  },
  "peerDependencies": {
    "stylelint": "^9.2.0"
  },
  "license": "MIT",
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": ["/node_modules/"]
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "package.json": [
      "prettier-package-json --write",
      "prettier package.json --write",
      "git add"
    ],
    ".babelrc": ["prettier .babelrc --write", "git add"]
  }
}
