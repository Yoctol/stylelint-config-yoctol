{
  "name": "stylelint-config-yoctol",
  "description": "Yoctol's stylelint config",
  "license": "MIT",
  "author": "Yoctol Info.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoctol/stylelint-config-yoctol.git"
  },
  "version": "1.1.1",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint src .babelrc.js",
    "test": "npm run lint && npm run testonly",
    "testonly": "jest",
    "testonly:watch": "jest --watch",
    "preversion": "npm run test"
  },
  "dependencies": {
    "css-property-sort-order-smacss": "^2.1.2",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1"
  },
  "peerDependencies": {
    "stylelint": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-yoctol-base": "^0.17.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "prettier-package-json": "^1.6.0",
    "rimraf": "^2.6.2",
    "stylelint": "^9.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "resetModules": true,
    "resetMocks": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
